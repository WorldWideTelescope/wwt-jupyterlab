{
  "author": "The AAS WorldWide Telescope Team <wwt@aas.org>",
  "bugs": {
    "email": "wwt@aas.org",
    "url": "https://github.com/WorldWideTelescope/wwt-research-kit/issues"
  },
  "name": "@wwtelescope/jupyterlab",
  "version": "0.0.0-dev.0",
  "description": "AAS WorldWide Telescope in JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/WorldWideTelescope/wwt-research-kit",
  "license": "MIT",
  "files": [
    "dist/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/WorldWideTelescope/wwt-research-kit.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.2.6 || ^3.0.0",
    "@jupyterlab/apputils": "^2.2.6 || ^3.0.0",
    "@jupyterlab/launcher": "^2.2.6 || ^3.0.0",
    "@jupyterlab/services": "^5.2.5 || ^6.0.0",
    "@jupyterlab/ui-components": "^2.2.4 || ^3.0.0",
    "@lumino/algorithm": "^1.3.3",
    "@lumino/messaging": "^1.4.3",
    "@lumino/widgets": "^1.14.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
